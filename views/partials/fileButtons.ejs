<div class="m-4">
  <button type="button" class="btn btn-primary" onclick="writeFile()">Write message.txt</button>
  <button type="button" class="btn btn-secondary" onclick="deleteFile()">Delete message.txt</button>
</div>
<script>
  function writeFile() {
    fetch("/fs/writeFile", { method: "post" }).then((response) => {
      console.log(response);
    });
  }
  function deleteFile() {
    fetch("/fs/deleteFile", { method: "delete" }).then((response) => {
      console.log(response);
    });
  }
</script>
